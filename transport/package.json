    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.2",
    "react-confetti": "^6.1.0",
    "react-hook-form": "^7.43.8",
    "react-materialize": "^3.10.0",
    "sweetalert2": "^11.7.3",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21"
  }
